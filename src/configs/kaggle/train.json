{
  "name": "train",
  "n_gpu": 1,
  "sr": 16000,
  "preprocessing": {
    "sr": 16000,
    "spectrogram": {
      "type": "LFCC",
      "args": {
      }
    },
    "log_spec": true
  },
  "arch": {
    "type": "LCNN",
    "args": {
      "channels": [64, 32, 48, 64, 32, 32]
    }
  },
  "data": {
    "train": {
      "batch_size": 256,
      "num_workers": 4,
      "datasets": [
        {
          "type": "AsDataset",
          "args": {
            "part": "train",
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA",
            "index_dir": "/kaggle/working/",
            "max_audio_len": 64000
          },
          "module": "src.datasets"
        }
      ]
    },
    "dev": {
      "batch_size": 256,
      "num_workers": 4,
      "datasets": [
        {
          "type": "AsDataset",
          "args": {
            "part": "dev",
            "data_dir": "/kaggle/input/asvpoof-2019-dataset/LA/LA",
            "index_dir": "/kaggle/working/",
            "max_audio_len": 64000
          },
          "module": "src.datasets"
        }
      ]
    }
  },
  "optimizer": {
    "type": "AdamW",
    "args": {
      "lr": 2e-4,
      "weight_decay": 0.1
    }
  },
  "loss": {
    "type": "CrossEntropyLoss",
    "module": "torch.nn",
    "args": {}
  },
  "lr_scheduler": {
    "type": "OneCycleLR",
    "args": {
      "steps_per_epoch": 100,
      "epochs": 20,
      "anneal_strategy": "cos",
      "max_lr": 2e-4,
      "pct_start": 0.1
    }
  },
  "trainer": {
    "epochs": 20,
    "save_dir": "saved/",
    "save_period": 5,
    "verbosity": 2,
    "monitor": "min dev_loss",
    "early_stop": 500,
    "visualize": "wandb",
    "wandb_project": "dla-5-as",
    "len_epoch": 100,
    "grad_norm_clip": 10
  }
}